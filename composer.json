{
    "name": "sevenlinx/auth0-driver",
    "description": "Auth0 Authentication driver",
    "type": "library",
    "require": {
        "php": "^8.0",
        "auth0/auth0-php": "^7.7",
        "illuminate/contracts": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/support": "^8.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "auth0/login": "^6.4",
        "orchestra/testbench": "^6.0",
        "phpstan/phpstan": "^0.12.83"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Jay Are Galinada",
            "email": "jayaregalinada@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SevenLinX\\Auth\\Auth0\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SevenLinX\\Auth\\Auth0\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SevenLinX\\Auth\\Auth0\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true
}
